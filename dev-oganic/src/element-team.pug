extends includes/layout
include includes/mixin

block content
  article.entry
    .entry-content
      //- Page title
      .small-bg-faq.mb-px-lg-100.mb-px-md-80.mb-px-60
        .container
          +hero-bg(
            'card-post-style',
            'col-lg-12',
            false,
            {
              tag:"Our",
              title:"Team",
              content:"Lorem ipsum dolor sit amet, consectetur adipiscing elit. Maecenas vestibulum urna augue. Duis sit maximus commodo hendrerit lorem maximus mi.",
              image:"assets/images/faq/fa1.jpg",
              alt:"faq hero image"
            })

      // team 
      .container
        .row.large-gutters
          .col-lg-3.mb-7
            //- Elements nav
            include includes/elements-nav
          .col-lg-9.mb-7
            h1.mb-5 Our Team Members
            +team(
              'card-post-style',
              'col-lg-6 mb-px-lg-0 mb-px-md-50 mb-px-40',
              true,
              {
                image:"assets/images/team/t2.jpg",
                alt:'t2 image',
                designation:"Managing Director",
                title:"Marsh James"
              },
              {
                image:"assets/images/team/t3.jpg",
                alt:'t3 image',
